{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "types": [
      "@cloudflare/workers-types"
    ],
    "paths": {
      "~/*": [
        "./src/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
}